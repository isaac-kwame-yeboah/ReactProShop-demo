{
  "name": "react_proshop",
  "version": "1.0.0",
  "description": "eCommerce application built with mern stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js -i",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaac-kwame-yeboah/ReactProShop-demo.git"
  },
  "author": "Yeboah Isaac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isaac-kwame-yeboah/ReactProShop-demo/issues"
  },
  "homepage": "https://github.com/isaac-kwame-yeboah/ReactProShop-demo#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0"
  }
